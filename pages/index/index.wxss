/**index.wxss**/
/*<!--map模块-->*/
.ql-map {
  width: 100%;
  height: 100%;
}
/*<!--扫码弹出层-->*/
modal {
  text-align: left;
}
 /*<!--newm模块-->*/
.ql-new {
  width: 690rpx;
  height: 100rpx;
  background-color: #fff;
  position: fixed;
  top: 30rpx;
  left: 30rpx;
  box-shadow: 0 2px 5px #ccc;
  line-height: 100rpx;
}
.ql-new image {
  float: left;
  display: block;
  height: 50rpx;
  width: 50rpx;
  margin: 25rpx 20rpx 0 30rpx;
}
.ql-new .right{
  float: right;
  margin-right: 30rpx;
  color: #ccc;
}
.ql_scan{
  position: fixed;
  bottom:180rpx;
  margin: 0 auto;
  width:150px;
  height:55px;
 overflow-x: scroll
}
